    "Include": {
      "type": "object",
      "required": [ "href" ],
      "description": "Refer to http://www.w3.org/2004/08/xop/include for original definition",
      "properties": {
        "otherAttributes": { "type": "object" },
        "Any": {
          "type": "array",
          "items": { "type": "object" },
          "minItems": 0
        },
        "href": {
          "type": "string",
          "format": "uri"
        }
      }
    },
